PORT=3000

# Database configuration
DB_HOST=mysql
DB_NAME=funerarias_db
DB_PORT=3306
# Application DB user (used by the app pool)
DB_USER_APP=app_user
DB_PASSWORD_APP=app_password

# Admin DB user (used by the admin pool)
DB_USER_ADMIN=admin_funeraria
DB_PASSWORD_ADMIN=Admin123!

# Docker configuration
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=funerarias_db
MYSQL_CHARSET=utf8mb4

# Frontend configuration
CORS_ORIGIN=http://localhost:5173

# Session configuration
SESSION_SECRET=your_session_secret