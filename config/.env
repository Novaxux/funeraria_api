MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=funeraria_db_v2
MYSQL_CHARSET=utf8mb4

# Frontend configuration
CORS_ORIGIN=http://localhost:5173

# Session configuration
SESSION_SECRET=your_session_secret